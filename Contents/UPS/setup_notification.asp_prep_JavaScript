    function isTypeEmail(type)
    {
     if (type == "EMail")
     {
      document.write('<table width=100% align=center cellspacing=1 cellpadding=5 border=0 class=dialogCellLine>');
      document.write('<tr class=dialogColHeader>');
      document.write('<td align=center colspan=2>');
      document.write('<b>Cx_0dummy</b>');
      document.write('</td>');
      document.write('</tr>');
      document.write('<tr class=dialogLineItem>');
   document.write('<td align=center>');
   document.write('Cx_0dummy');
   document.write('</td>');
   document.write('<td align=center width=50%>');
   document.write('<input maxLength="50" name="Server" size="20" value="Cx_0dummy">');
   document.write('</td>');
     document.write('</tr>');
     document.write('<tr class=dialogLineItem>');
   document.write('<td align=center>');
   document.write('Cx_0dummy');
   document.write('</td>');
   document.write('<td align=center>');
   document.write('<input maxLength="50" name="From_Address" size="20" value="Cx_0dummy">');
   document.write('</td>');
   document.write('</tr>');



















   document.write('</table>');
   document.write('<br>');
  }
  else if (type == "SNMP")
  {
   document.write('<table width=100% align=center cellspacing=1 cellpadding=5 border=0 class=dialogCellLine>');
      document.write('<tr class=dialogColHeader>');
      document.write('<td align=center colspan=2>');
      document.write('<b>Cx_0dummy</b>');
      document.write('</td>');
      document.write('</tr>');
      document.write('<tr class=dialogLineItem>');
   document.write('<td align=center>');
   document.write('Cx_0dummy');
   document.write('</td>');
   document.write('<td align=center width=50%>');
   document.write('<input maxLength="50" name="Community_String" size="20" value="Cx_0dummy">');
   document.write('</td>');
     document.write('</tr>');
   document.write('</table>');
   document.write('<br>');
  }
    }
         
