  && document.formSetNotificationRecip.Critical_List_3.value == "" && document.formSetNotificationRecip.Critical_List_4.value == ""
  && document.formSetNotificationRecip.Critical_List_5.value == "")
  {
   if (!confirm("Cx_0dummy"))
    return false;
